node {
    def name = "1111"

    stage('prep') {
      println 'prep' 
    }

    stage('build') {
      println 'build'

      // Prameter
      println "$println: {params.isBoolean}"
      echo "echo: ${params.isBoolean}"
      println "name: ${name}"
      name = "cetauri"

    }

    stage('test') {
      println 'test'
    }

    stage('deploy') {
      println 'deploy'
      println "name: ${name}"
    }
}